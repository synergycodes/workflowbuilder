    "anymatch": "^3.1.3",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0-rc.1",
    "express": "^5.1.0",
    "javascript-obfuscator": "^4.1.1",
    "vite": "^6.0.7",
    "vite-bundle-analyzer": "^0.17.1",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.0.4"
