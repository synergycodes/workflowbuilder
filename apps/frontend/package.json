    "@workflow-builder/icons": "workspace:*",
    "@xyflow/react": "catalog:",
    "ace-builds": "^1.43.4",
    "ajv": "^8.0.0",
    "clsx": "^2.1.1",
    "elkjs": "^0.9.3",
    "html-to-image": "1.11.11",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.5",
    "immer": "^10.1.1",
    "jspdf": "^3.0.1",
    "libavoid-js": "0.4.0-beta.1",
    "notistack": "^3.0.1",
    "react": "^19.1.0",
    "react-ace": "^14.0.1",
