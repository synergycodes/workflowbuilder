{
  "name": "@workflow-builder/frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently 'pnpm typecheck:watch' 'vite'",
    "fix-worker": "tsx scripts/fix-worker/fix-worker.ts",
    "build": "vite build && pnpm fix-worker",
    "build:analyze": "ANALYZE=true vite build",
    "preview-build": "tsx scripts/preview-build.ts",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch --pretty",
